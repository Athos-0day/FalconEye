{
    "id": null,
    "title": "Navigation Systems Monitoring",
    "tags": ["navigation", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 1,
    "refresh": "3s",
    "time": {
      "from": "now-5m",
      "to": "now"
    },
    "panels": [
      {
        "type": "timeseries",
        "title": "Air Speed (km/h)",
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
        "datasource": "InfluxDB",
        "targets": [
          {
            "refId": "A",
            "query": "from(bucket: \"mybucket\") |> range(start: -5m) |> filter(fn: (r) => r._measurement == \"navigation\" and r._field == \"airspeed\")"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "Latitude",
        "gridPos": { "x": 0, "y": 8, "w": 6, "h": 6 },
        "datasource": "InfluxDB",
        "targets": [
          {
            "refId": "B",
            "query": "from(bucket: \"mybucket\") |> range(start: -5m) |> filter(fn: (r) => r._measurement == \"navigation\" and r._field == \"latitude\")"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "Longitude",
        "gridPos": { "x": 6, "y": 8, "w": 6, "h": 6 },
        "datasource": "InfluxDB",
        "targets": [
          {
            "refId": "C",
            "query": "from(bucket: \"mybucket\") |> range(start: -5m) |> filter(fn: (r) => r._measurement == \"navigation\" and r._field == \"longitude\")"
          }
        ]
      },
      {
        "type": "geomap",
        "title": "Flight Path",
        "gridPos": { "x": 0, "y": 16, "w": 24, "h": 10 },
        "datasource": "InfluxDB",
        "targets": [
          {
            "refId": "A",
            "query": "from(bucket: \"mybucket\") |> range(start: -5m) |> filter(fn: (r) => r._measurement == \"navigation\" and (r._field == \"latitude\" or r._field == \"longitude\")) |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")"
          },
          {
            "refId": "B",
            "query": "from(bucket: \"mybucket\") |> range(start: -5m) |> filter(fn: (r) => r._measurement == \"navigation\" and (r._field == \"latitude\" or r._field == \"longitude\")) |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\") |> sort(columns:[\"_time\"], desc:true) |> limit(n:1)"
          }
        ],
        "mapCenter": { "lat": 48.8566, "lon": 2.3522, "zoom": 4 },
        "layers": [
          {
            "type": "markers",
            "config": {
              "latField": "latitude",
              "lonField": "longitude",
              "showTooltip": true,
              "color": {
                "fixedColor": "blue"
              }
            }
          },
          {
            "type": "markers",
            "config": {
              "latField": "latitude",
              "lonField": "longitude",
              "showTooltip": true,
              "color": {
                "fixedColor": "red"
              },
              "pointSize": 10
            }
          }
        ]
      }      
    ]
  }
  